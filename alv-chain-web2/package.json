  "scripts": {
    "build": "env-cmd --silent nest build",
    "format": "env-cmd --silent prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "env-cmd --silent nest start",
    "start:dev": "env-cmd --silent nest start --watch",
    "start:debug": "env-cmd --silent nest start --debug --watch",
    "start:prod": "env-cmd --silent node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "migration:create": "env-cmd --silent typeorm-ts-node-commonjs migration:create",
    "migration:run": "env-cmd --silent ts-node -r tsconfig-paths/register ./src/script/typeorm_migration_run.ts",
    "migration:rollback": "env-cmd --silent ts-node -r tsconfig-paths/register ./src/script/typeorm_migration_rollback.ts",
    "migration:rollbackAll": "env-cmd --silent ts-node -r tsconfig-paths/register ./src/script/typeorm_migration_rollback_all.ts",
    "entity:create": "env-cmd --silent typeorm-ts-node-commonjs entity:create",
    "seeder:run": "env-cmd --silent node dist/seeder",
    "seeder:refresh": "env-cmd --silent node dist/seeder --refresh",
    "seeder:run:dev": "env-cmd --silent ts-node -r tsconfig-paths/register src/seeder",
    "seeder:refresh:dev": "env-cmd --silent ts-node -r tsconfig-paths/register src/seeder --refresh"
  },
  "dependencies": {
    "@bull-board/api": "^5.15.1",
    "@bull-board/express": "^5.15.1",
    "@google-cloud/local-auth": "2.1.0",
    "@nestjs-modules/mailer": "^1.11.2",
    "@nestjs/bull": "^10.1.0",
    "@nestjs/common": "^10.0.0",
    "@nestjs/config": "^3.2.0",
    "@nestjs/core": "^10.0.0",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/passport": "^10.0.3",
    "@nestjs/platform-express": "^10.0.0",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@types/multer": "^1.4.11",
    "argon2": "^0.40.1",
    "axios": "^1.7.7",
    "bull": "^4.12.2",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "dot": "^1.1.3",
    "dotenv": "^16.4.5",
    "env": "^0.0.2",
    "env-cmd": "^10.1.0",
    "generate-password": "^1.7.1",
    "googleapis": "95",
